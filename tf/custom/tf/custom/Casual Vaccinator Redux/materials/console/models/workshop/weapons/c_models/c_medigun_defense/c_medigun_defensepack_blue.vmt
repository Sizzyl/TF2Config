"VertexlitGeneric"
{
	"$baseTexture" "console/models/workshop/weapons/c_models/c_medigun_defense/c_medigun_defensepack_blue"
	"$bumpmap" "console/models/workshop/weapons/c_models/c_medigun_defense/c_medigun_defensepack_normal"
	"$bumpmapalphaphongmask" "1"
	"$phong" "1"
	"$phongexponent" "4"
	"$phongboost" ".5"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.25 1 4]"
	"$selfIllum" "1"
	"$yellow" "0"
	"$rimlight" "0"
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
// This is commented out because it conflicts with the Sine proxy.
//		"BurnLevel"
//		{
//			"resultVar" "$detailblendfactor"
//		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
	}
}
